        <activity android:name=".view.add_code"></activity>
        <activity android:name=".view.delete_inside_annonce" />
        <activity android:name=".view.delete_first" />
        <activity android:name=".view.delete_annonce" />
        <activity android:name=".view.delete" />
        <activity android:name=".view.Users" />
        <activity android:name=".view.add_shop" />
        <activity android:name=".view.Add_annonce" />
        <activity android:name=".view.add_first_category" />
        <activity android:name=".view.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.MainActivity" />
    </application>

</manifest>